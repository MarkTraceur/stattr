<div id="stattr-js-event-home">
  <p class="stattr-style-title" id="stattr-js-event-home-activity"></p>
  <p id="stattr-js-event-home-descr"></p>
  <input id="stattr-js-add-results" type="button" value="Add Results" />
  <p class="stattr-style-subhead">Event Statistics</p>
  <table id="stattr-js-event-stats" class="stattr-style-result-table">
    <tr class="stattr-js-event-stat-repeater">
      <th class="stattr-js-event-stat-header stattr-style-variable-listing" />
      <td class="stattr-js-event-stat-repeater-average stattr-style-variable-listing" />
      <td class="stattr-js-event-stat-repeater-high stattr-style-variable-listing" />
      <td class="stattr-js-event-stat-repeater-low stattr-style-variable-listing" />
    </tr>
  </table>
  <p class="stattr-style-subhead">Recent Results</p>
  <table class="stattr-js-event-home-results-table stattr-style-result-table">
    <tr class="stattr-js-event-home-result-repeater">
      <th class="stattr-js-event-home-variable-repeater stattr-style-variable-listing"></th>
      <td class="stattr-js-event-home-variable-repeatee stattr-style-variable-listing"></td>
    </tr>
  </table>
</div>
